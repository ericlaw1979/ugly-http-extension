{
  "manifest_version": 2,

  "name": "Ugly HTTP",
  "description": "Makes HTTP pages conspicuous.",
  "version": "0.1",
  "icons": {
    "128": "images/extensionIcon-128px.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["ugly.css"]
    }
  ]
}
